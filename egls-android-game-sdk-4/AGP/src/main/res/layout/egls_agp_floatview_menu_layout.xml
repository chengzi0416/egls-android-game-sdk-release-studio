<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/floatview_main_menu_box"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:color/transparent"
    android:orientation="horizontal" >

    <LinearLayout
        android:id="@+id/floatview_main_linearlayout_menu"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:background="@drawable/kr_23_r"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <RelativeLayout
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@android:color/transparent" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:contentDescription="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/kr_51" />

                <com.egls.platform.views.AGPAutoScaleMinSizeTextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:contentDescription="egls_agp_always_sp_7"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/egls_agp_dp_6"
                    android:paddingRight="@dimen/egls_agp_dp_6"
                    android:text="@string/egls_agp_floate_text_1"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/floatview_main_textview_usercenter"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="center" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/floatview_menu_rl_agreement"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:contentDescription="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/kr_84" />

                <com.egls.platform.views.AGPAutoScaleMinSizeTextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:contentDescription="egls_agp_always_sp_7"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/egls_agp_dp_6"
                    android:paddingRight="@dimen/egls_agp_dp_6"
                    android:text="@string/egls_agp_floate_text_2"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/floatview_main_textview_agreement"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="center" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/floatview_menu_rl_op"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:contentDescription="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/kr_85" />

                <com.egls.platform.views.AGPAutoScaleMinSizeTextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:contentDescription="egls_agp_always_sp_7"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/egls_agp_dp_6"
                    android:paddingRight="@dimen/egls_agp_dp_6"
                    android:text="@string/egls_agp_floate_text_3"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/floatview_main_textview_op"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="center" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/floatview_menu_rl_navercafe"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:background="@android:color/transparent"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:gravity="center_horizontal"
                android:orientation="vertical" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:contentDescription="@null"
                    android:scaleType="fitXY"
                    android:src="@drawable/kr_83" />

                <com.egls.platform.views.AGPAutoScaleMinSizeTextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:contentDescription="egls_agp_always_sp_7"
                    android:gravity="center_horizontal"
                    android:paddingLeft="@dimen/egls_agp_dp_6"
                    android:paddingRight="@dimen/egls_agp_dp_6"
                    android:text="@string/egls_agp_floate_text_4"
                    android:textColor="#696969"
                    android:textSize="7sp" />
            </LinearLayout>

            <TextView
                android:id="@+id/floatview_main_textview_navercafe"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="center" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>